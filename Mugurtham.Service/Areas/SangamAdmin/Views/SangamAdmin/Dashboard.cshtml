@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Areas/SangamAdmin/Views/Shared/LayoutSangamAdmin.cshtml";
}



<div class="col-sm-12" data-ng-init="getChartData();">
    <h3>Sangam Admin Dashboard</h3>
    <hr />
</div>



<div class="row">
    <div class="col-md-3 col-sm-12 col-xs-12">
        <div class="panel panel-primary text-center no-boder bg-color-green ">
            <div class="panel-body">
                <i class="fa fa-bar-chart-o fa-5x"></i>
                <h3>8,457</h3>
            </div>
            <div class="panel-footer back-footer-green">
                Total Logins

            </div>
        </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
        <div class="panel panel-primary text-center no-boder bg-color-blue">
            <div class="panel-body">
                <i class="fa fa-shopping-cart fa-5x"></i>
                <h3>52,160 </h3>
            </div>
            <div class="panel-footer back-footer-blue">
                Total Profile full viewed

            </div>
        </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
        <div class="panel panel-primary text-center no-boder bg-color-red">
            <div class="panel-body">
                <i class="fa fa fa-comments fa-5x"></i>
                <h3>15,823 </h3>
            </div>
            <div class="panel-footer back-footer-red">
                Active Profiles

            </div>
        </div>
    </div>
    <div class="col-md-3 col-sm-12 col-xs-12">
        <div class="panel panel-primary text-center no-boder bg-color-brown">
            <div class="panel-body">
                <i class="fa fa-users fa-5x"></i>
                <h3>36,752 </h3>
            </div>
            <div class="panel-footer back-footer-brown">
                Total Profiles

            </div>
        </div>
    </div>
</div>



<h3>This year sales by Month</h3>
<div class="col-lg-12">
    <div class="col-lg-9" id="divPieSales" style="height: 300px; width: 400px;"></div>
    <div class="col-lg-3" id="divBarReport" style="height: 300px; width: 350px;"></div>
</div>
<div class="col-lg-12">
    <div class="col-lg-3" id="divStackStar" style="height: 300px; width: 750px;"></div>
</div>

