@{
    ViewBag.Title = "List";
    Layout = "~/Areas/User/Views/Shared/LayoutUser.cshtml";
}
<p class="lead">User List</p>
<div class="col-lg-12" data-ng-init="getAllUsers();">
    <ul class="breadcrumb">
        <li class="active">List</li>
    </ul>
    <button id="btnSave" name="btnSave" class="btn btn-primary" onclick="javascript:window.location='#UserForm';">Add User</button>
</div>
<div class="col-lg-12">
    <br />
</div>
<table class="mugurthamTable table-striped table-bordered table-hover FormShadow">
    <thead>
        <tr>
            <th>Name</th>
            <th>Sangam</th>
            <th>Role</th>
            <th>Theme</th>
            <th>Locale</th>
            <th>IsActivated</th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="User in AllUsers">
            <td><a href="#UserForm/{{User.ID}}">{{User.Name}}</a></td>
            <td><a href="#UserForm/{{User.ID}}">{{User.SangamID}}</a></td>
            <td><a href="#UserForm/{{User.ID}}">{{User.RoleID}}</a></td>
            <td><a href="#UserForm/{{User.ID}}">{{User.ThemeID}}</a></td>
            <td><a href="#UserForm/{{User.ID}}">{{User.LocaleID}}</a></td>
            <td><a href="#UserForm/{{User.ID}}">{{User.IsActivated}}</a></td>
            <td>{{User.Description}}</td>
        </tr>
    </tbody>
</table>




