@{
    Mugurtham.Core.Login.LoggedInUser objLoggedIn = (Mugurtham.Core.Login.LoggedInUser)Session["LoggedInUser"];
}
<!--
    THIS PAGE IS THE LAYOUT PAGE FOR THE PROFILE REGISTRATION MODULE
    -->
<!-- Rending Body from child page-->
<div>
    <ul class="breadcrumb" >
        <li><a href="@objLoggedIn.HomePagePath" translate="">{{ 'GENHOME' | translate }}</a></li>
        <li><a href="#ProfileID">{{ 'GENSRCH' | translate }}</a></li>
        <li><a href="#FullView/{{globalProfileID}}">{{ 'GENVIEW' | translate }}</a></li>
        <li class="active">{{ 'GENPROF' | translate }}</li>
    </ul>
</div>
<div class="panel panel-primary bs-example FormShadow">
    <div class="panel-heading" >
        <h3 class="panel-title" >{{ 'LYTPRFREGFOR' | translate}} {{globalProfileID}}</h3>
        @RenderSection("featured", required: false)
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-12">

                <!-- Tabs-->
                <div>
                    <ul id="navBarProfile" class="nav nav-tabs">
                        <li class="active"><a href="#BasicInfo" ng-click="setActiveTab('tabBasicInfo');" id="tabBasicInfo"><i class="fa fa-info-circle" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABBI' | translate }}</a></li>
                        <li class=""><a href="#Career" ng-click="setActiveTab('tabCareer');" id="tabCareer"><i class="fa fa-briefcase" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABCAR' | translate }}</a></li>
                        <li class=""><a href="#Contact" ng-click="setActiveTab('tabContact');" id="tabContact"><i class="fa fa-volume-control-phone" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABCONT' | translate }}</a></li>
                        <li class=""><a href="#Family" ng-click="setActiveTab('tabFamily');" id="tabFamily"><i class="fa fa-home" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABFAM' | translate }}</a></li>
                        <li class=""><a href="#Location" ng-click="setActiveTab('tabLocation');" id="tabLocation"><i class="fa fa-map-marker" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABLOC' | translate }}</a></li>
                        <li class=""><a href="#Reference" ng-click="setActiveTab('tabReference');" id="tabReference"><i class="fa fa-hand-o-right" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABREF' | translate }}</a></li>
                        <li class=""><a href="#Horoscope" ng-click="setActiveTab('tabHoroscope');" id="tabHoroscope"><i class="fa fa-balance-scale" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABHORO' | translate }}</a></li>
                        <li class=""><a href="#Photo" ng-click="setActiveTab('tabPhoto');" id="tabPhoto"><i class="fa fa-camera" style="font-size:18px"></i>&nbsp; {{ 'LYTPRFTABPHO' | translate }}</a></li>
                        <!-- <li  class=""><a href="#Album" ng-click="setActiveTab('tabAlbum');" id="tabAlbum"><i class="fa fa-user" style="font-size:18px"></i>&nbsp; Album</a></li>-->
                    </ul>
                </div>
                <br />
                @RenderBody()

            </div>
        </div>
    </div>
</div>
