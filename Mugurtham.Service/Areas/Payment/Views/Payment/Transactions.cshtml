
@{
    ViewBag.Title = "Transactions";
    Layout = "~/Areas/Payment/Views/Shared/LayoutPayment.cshtml";
    Mugurtham.Core.Login.LoggedInUser objLoggedIn = (Mugurtham.Core.Login.LoggedInUser)Session["LoggedInUser"];
}



<div id="divTransactionList" ng-init="getAllPaymentTransactions();">
    <p class="lead">Payment Transaction List</p>
    <hr />
    <div class="col-lg-12" data-ng-init="getAllSangamUsers('@objLoggedIn.sangamID');">
        <ul class="breadcrumb">
            <li><a href="@objLoggedIn.HomePagePath">{{ 'GENHOME' | translate }}</a></li>
            <li class="active">User List</li>
        </ul>
    </div>

    <table id="tblTransactions" class="display" cellspacing="0">
        <thead>
            <tr>
                <th>TransactionID</th>
                <th>Date</th>
                <th>Amount</th>
                <th>Mode</th>
                <th>For</th>
                <th>Notes</th>
            </tr>
        </thead>
        <tfoot>
            <tr>
                <th>TransactionID</th>
                <th>Date</th>
                <th>Amount</th>
                <th>Mode</th>
                <th>For</th>
                <th>Notes</th>
        </tfoot>
    </table>
</div>
 